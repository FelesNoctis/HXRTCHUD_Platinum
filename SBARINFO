				// Background panels
					/*
					*	Quad Damage Display
					*	This is a little awkward due to how SBARINFO considers a value to be "Greater Than Or Equal To".
					*	So we need to specifically declare "1", which is only Berserk, as blank.
					*/
					InInventory "HX_Acid_Eri_PSTR", 2 { DrawImage "HXAEQDNO", -122, 2; }
					else InInventory "HX_Acid_Eri_PSTR", 1 { /* Nothing to see here */ }
					else { DrawImage "HXAEQDNO", -122, 2; }
					// DrawImage "HXINVUNO", -98, 2; (Affected by settings)
					// DrawImage "HXBLURNO", -74, 2; (Affected by settings)
