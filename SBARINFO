				else { DrawString HXGENERALFONTM, HXRTCGold, "MODE: Brutal Doom 64 (Purist)", 2, 54, 0, alignment(left), drawshadow(1,1); }
			}
			else InInventory "HXMOD_CombinedArms", 1 { DrawString HXGENERALFONTM, HXRTCGold, "MODE: Combined_Arms", 2, 54, 0, alignment(left), drawshadow(1,1); }
